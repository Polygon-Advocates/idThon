{
  "name": "idthon",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "repository": {
    "url": "https://github.com/wefa-tech/idThon"
  },
  "scripts": {
    "initialize": "pnpm recursive run initialize",
    "dev": "concurrently -n verifier,api,generator,client -c cyan,green,yellow,magenta 'cd packages/verifier && pnpm dev' 'cd packages/api && pnpm dev' 'pnpm dev:creature-generator' 'cd packages/client && pnpm dev'",
    "dev:api": "pnpm --filter 'api' run dev",
    "dev:client": "pnpm --filter 'client' run dev",
    "dev:verifier": "pnpm --filter 'verifier' dev",
    "dev:creature-generator": "cd packages/creature-generator && python3 main.py",
    "test": "pnpm recursive run test",
    "build": "pnpm recursive run build"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.46.1",
    "@typescript-eslint/parser": "5.46.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "17.0.38",
    "concurrently": "^8.0.1",
    "dotenv": "16.1.0",
    "identity-obj-proxy": "3.0.0",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.3",
    "eslint": "8.29.0",
    "rimraf": "^3.0.2",
    "typescript": "^5.0.4",
    "wait-port": "^1.0.4"
  },
  "engines": {
    "node": "20.x",
    "pnpm": "8.x"
  },
  "packageManager": "pnpm@8.6.0"
}
